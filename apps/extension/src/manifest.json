{
  "manifest_version": 3,
  "name": "LocatorJS",
  "icons": {
    "128": "icon-128.png"
  },
  "action": {
    "default_title": "Click to run LocatorJS on this page"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["contentScript.bundle.js"],
      "run_at": "document_start"
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "client.bundle.js",
        "hook.bundle.js",
        "content.styles.css",
        "icon-128.png",
        "icon-34.png"
      ],
      "matches": ["<all_urls>"]
    }
  ]
}
